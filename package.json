{
  "name": "searchbox",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build remote --prod && cp docs/index.html index.html",
    "buildLocal": "ng build local --prod && mv docs/index.local.html docs/index.html && cp docs/index.html index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/searchbox.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/searchbox.umd.min.js",
    "buildlib": "rm -rf dist && npm run transpile && npm run package && npm run minify && mkdir dist/scss && cp src/assets/app.scss dist/scss/",
    "doc": "typedoc --out ./docs --mode file --target ES6 ./src/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/pwa": "^0.8.5",
    "@angular/router": "^6.1.10",
    "@angular/service-worker": "^6.1.10",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "@ng-select/ng-select": "^2.0.0",
    "angular2-uuid": "^1.1.1",
    "bootstrap": "^4.1.0",
    "core-js": "^2.4.1",
    "ngx-advanced-searchbox": "0.2.1",
    "ngx-highlightjs": "^2.0.2",
    "ngx-input-autosize": "^1.1.7",
    "ngx-mask": "2.7.3",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^6.1.0",
    "ts-keycode-enum": "^1.0.4",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^6.2.5",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.7.2",
    "@angular-devkit/build-angular": "~0.8.5"
  }
}
